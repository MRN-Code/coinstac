{
  "id": "single-shot-test-id",
  "meta": {
    "repository": "https://github.com/MRN-Code/decentralized-single-shot-ridge-regression",
    "description": "Decentralized ridge regression.",
    "name": "Single-Shot Ridge Regression",
    "tags": [
      "regression"
    ],
    "version": "v1.1.10",
    "controller": "single"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "mysql",
    "command": ["python", "/computation/comp1.py"],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstac/coinstac-decentralized-test",
      "command": ["python", "/computation/comp2.py"]
    },
    "input": {
      "lambda":
      {
        "defaultValue": 0,
        "label": "Lambda",
        "max": 1,
        "min": 0,
        "step": 0.05,
        "type": "number"
      },
      "covariates":
      {
        "label": "Covariates",
        "type": "array",
        "items": ["boolean", "number"]
      }
    },
    "output": {
      "type": "function",
      "command": "src/remote.js",
      "betaVector": {
        "type": "array",
        "label": "Beta Vector",
        "items": "number"
      },
      "localCount": {
        "type": "array",
        "label": "Local Count",
        "items": "number"
      },
      "localMeanY": {
        "type": "number",
        "label": "Local Mean Y"
      },
      "rSquared": {
        "type": "number",
        "label": "R squared"
      },
      "tValue": {
        "type": "number",
        "label": "T value"
      },
      "pValue": {
        "type": "number",
        "label": "P Value"
      },
      "biasedX": {
        "type": "number",
        "label": "Biased Xs"
      },
      "y": {
        "type": "number",
        "label": "Y"
      }
    }
  }
}