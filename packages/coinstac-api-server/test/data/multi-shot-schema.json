{
  "id": "multi-shot-test-id",
  "meta": {
    "repository": "https://github.com/MRN-Code/decentralized-laplacian-ridge-regression",
    "description": "Iterative decentralized ridge regression.",
    "name": "Multi-Shot Ridge Regression",
    "tags": [
      "regression"
    ],
    "version": "3.1.11",
    "controller": "decentralized"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "mongo",
    "command": ["python", "/computation/comp1.py"],
    "remote": {
      "type": "docker",
      "dockerImage": "spanta28/vbm_ynipype",
      "command": ["python", "/computation/comp2.py"]
    },
    "input": {
      "iterationCount":
      {
        "defaultValue": 250,
        "label": "Iteration count",
        "type": "number"
      },
      "lambda":
      {
        "defaultValue": 0,
        "label": "Lambda",
        "max": 1,
        "min": 0,
        "step": 0.05,
        "type": "number"
      },
      "covariates":
      {
        "defaultValue": "[{'name': 'Is Control?','type': 'boolean'}]",
        "label": "Covariates",
        "type": "array",
        "items": ["boolean", "number"]
      }
    },
    "output": {
      "betaVector": {
        "type": "array",
        "label": "Beta Vector",
        "items": "number"
      },
      "localCount": {
        "type": "array",
        "label": "Local Count",
        "items": "number"
      },
      "localMeanY": {
        "type": "number",
        "label": "Local Mean Y"
      },
      "rSquared": {
        "type": "number",
        "label": "R squared"
      },
      "tValue": {
        "type": "number",
        "label": "T value"
      },
      "pValue": {
        "type": "number",
        "label": "P Value"
      },
      "biasedX": {
        "type": "number",
        "label": "Biased Xs"
      },
      "y": {
        "type": "number",
        "label": "Y"
      }
    }
  }
}