{
  "name": "coinstac",
  "version": "2.0.0",
  "description": "Decentralized Analyses Made Easy",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "cross-spawn": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "fs-extra": "^0.30.0",
    "gh-pages": "^0.11.0",
    "github-markdown-css": "^2.3.0",
    "jsdoc": "^3.4.0",
    "lerna": "2.0.0-beta.32",
    "marked": "^0.3.5",
    "minami": "^1.1.1",
    "pre-commit": "^1.1.3",
    "swig-templates": "^2.0.1"
  },
  "scripts": {
    "bootstrap": "echo 'Deprecated! Please use \"npm run build\"'; exit 1;",
    "build": "lerna bootstrap && cd ./packages/coinstac-ui && npm run build-native",
    "build:server": "lerna bootstrap --ignore 'coinstac-{client-core,decentralized-algorithm-integration,simulator,storage-proxy,ui}'",
    "clean": "lerna clean",
    "clean:comps": "rm -rf packages/coinstac-computation-registry/node_modules/{laplacian-noise,decentralized-single-shot}-ridge-regression",
    "lint": "eslint '**/*.js' '**/bin/*'",
    "lintfix": "eslint --fix **/*.js",
    "publish": "lerna publish && node scripts/docs.js",
    "test": "lerna run test --concurrency=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRN-Code/coinstac.git"
  },
  "keywords": [
